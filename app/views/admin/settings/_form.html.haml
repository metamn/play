= form_for @admin_setting do |f|
  -if @admin_setting.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_setting.errors.count, "error")} prohibited this admin_setting from being saved:"
      %ul
        - @admin_setting.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :value
    = f.text_field :value
  .actions
    = f.submit 'Save'
